{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "outputDirectory": "dist",
  "functions": {
    "api/auth/**/*.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/wiki/**/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "dist/src/main.js"
    }
  ]
}